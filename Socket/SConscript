# -*- python -*-

Import('env')
import SatSCons

###########################################################################

sources = SatSCons.GlobSources()

SatSCons.StandardTargets(env)

SatSCons.Lib(env,
             library = 'Socket',
             sources = sources,
             LIBS = [ 'Utils' ])

SatSCons.Doxygen(env, extra_sources = [
    env.Dia2Png('SocketLibrary-classes.dia')
    ])
